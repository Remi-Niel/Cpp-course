//%default-actions quiet
%filenames parser
%scanner ../scanner/scanner.h

%token INTEGER
%token DOUBLE

%%

repl:
    '\n'
    {
        ACCEPT();
    }
    |
        'm' DOUBLE '\n'
    |
        's' DOUBLE '\n'
    |
        DOUBLE '\n'
    |
        DOUBLE INTEGER '\n'
    |
        repl repl
;







