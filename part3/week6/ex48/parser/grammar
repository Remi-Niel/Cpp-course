//%default-actions quiet
%filenames parser
%scanner ../scanner/scanner.h

%token INTEGER
%token DOUBLE
%token NEWLINE
%token MEAN_TOKEN
%token STDEV_TOKEN


%%

startrule:
    NEWLINE
    {
        ACCEPT();
    }
    |
        MEAN_TOKEN DOUBLE startrule 
    |
        STDEV_TOKEN DOUBLE startrule 
    |
        DOUBLE startrule 
    |
        DOUBLE INTEGER startrule 
;







